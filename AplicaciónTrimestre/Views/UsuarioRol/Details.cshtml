@model AplicaciónTrimestre.Models.usuariorol

@{
    ViewBag.Title = "Detalles Usuario Rol";
}

<h2>Detalles</h2>

<div>
    <h4>Usuario Rol</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @*@Html.DisplayNameFor(model => model.idUsuario)*@
            Usuario:
        </dt>

        <dd>
            @*@Html.DisplayFor(model => model.idUsuario)*@
            @AplicaciónTrimestre.Controllers.UsuarioRolController.NombreUsuario(Model.idUsuario)
        </dd>

        <dt>
            @*@Html.DisplayNameFor(model => model.idRol)*@
            Rol:
        </dt>

        <dd>
            @*@Html.DisplayFor(model => model.idRol)*@
            @AplicaciónTrimestre.Controllers.UsuarioRolController.DescripcionRol(Model.idRol)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.id }) |
    @Html.ActionLink("Volver a la lista", "Index")
</p>
